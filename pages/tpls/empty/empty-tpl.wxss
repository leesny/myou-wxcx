/*empty 缺省页  */

.empty-box {
  width: 100%;
  height: 100%;
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
  justify-content: center;
}

.empty-box .empty-icon {
  width: 220rpx;
  height: 180rpx;
}

.empty-box .empty-tip {
  font-size: 15px;
  font-weight: 600;
  margin-top: 40rpx;
  color: #898989;
  letter-spacing: 0.02em;
}

.empty-box navigator{
  width:200rpx;
  height:85rpx;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;
  margin-top:30rpx;
}

.btn-text {
  border: 1px solid #212121;
  color:#212121;
  width: 100%;
  margin-top: 30rpx;
  display: flex;
  align-items: center;
}

